<configuration>


  <system.web>
    <httpHandlers>
      <add verb="*" type="Rosanna.RosannaHttpRequestHandler" path="*"/>
    </httpHandlers>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>

  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
    <handlers>
      <add name="Rosanna" verb="*" type="Rosanna.RosannaHttpRequestHandler" path="*"/>
    </handlers>
  </system.webServer>

</configuration>
